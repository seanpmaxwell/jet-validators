{
  "name": "jet-validators",
  "version": "2.0.6",
  "description": "Fast, zero-dependency TypeScript validators and utilities for runtime checks, parsing, and lightweight object schema validation.",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js",
      "types": "./dist/types/utils/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "utils": ["dist/types/utils/index.d.ts"],
      "*": ["dist/types/index.d.ts"]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && tsc -p tsconfig.types.json",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "pre-publish": "mv README.md README-git && mv README-npm README.md",
    "post-publish": "mv README.md README-npm && mv README-git README.md",
    "test": "NODE_ENV=test vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpmaxwell/jet-validators.git"
  },
  "keywords": [
    "jet",
    "validator",
    "validators",
    "jet-validators",
    "validation",
    "function",
    "functions",
    "zod",
    "schema",
    "parse",
    "runtime-validation",
    "compile-time validation",
    "lightweight",
    "npm",
    "type-safety"
  ],
  "author": "sean maxwell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanpmaxwell/jet-validators/issues"
  },
  "homepage": "https://github.com/seanpmaxwell/jet-validators#readme",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^22.8.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.3.3",
    "prettier": "^3.7.4",
    "tslib": "^2.8.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.15"
  }
}
